<?xml version="1.0" encoding="utf-8"?>
<resources xmlns:android="http://schemas.android.com/apk/res/android">

    <style name="AppBaseTheme" parent="android:Theme.Light">
    </style>

    <style name="AppTheme" parent="AppBaseTheme">
    </style>
    
    <style name="textview1">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textSize">16.0dip</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:background">@null</item>
        <item name="android:layout_centerVertical">true</item>
    </style>
    
    <style name="textview2">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:padding">10.0dip</item>
        <item name="android:textSize">16.0dip</item>
        <item name="android:textColor">#ffffff</item>
        <item name="android:background">#9dd4db</item>
    </style>
    
     <style name="textview3">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:padding">10.0dip</item>
        <item name="android:textSize">16.0dip</item>
        <item name="android:textColor">#ababab</item>
    </style>
    
    <style name="button1">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">45.0dip</item>
        <item name="android:layout_margin">5.0dip</item>
        <item name="android:textSize">20.0dip</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:onClick">onClick</item>
        <item name="android:background">@drawable/selector_button_2</item>
    </style>
    
    <style name="edittext1">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">45.0dip</item>
        <item name="android:textSize">16.0dip</item>
        <item name="android:textColor">#000000</item>
        <item name="android:textColorHint">#dbd8d8</item>
        <item name="android:maxLength">20</item>
        <item name="android:padding">5.0dip</item>
        <item name="android:ems">10</item>
        <item name="android:drawableRight">@drawable/ic_input_clear</item>
        <item name="android:layout_marginLeft">10.0dip</item>
        <item name="android:layout_marginRight">10.0dip</item>
        <item name="android:background">@drawable/bg_frame</item>
    </style>
    
    <style name="randiobutton1">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textSize">16.0dip</item>
        <item name="android:textColor">#000000</item>
        <item name="android:textColorHint">#dbd8d8</item>
    </style>
    
    <style name="listview1">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:choiceMode">singleChoice</item>
        <item name="android:dividerHeight">1.0dip</item>
        <item name="android:fadeScrollbars">true</item>
        <item name="android:fadingEdge">none</item>
        <item name="android:fastScrollEnabled">true</item>
        <item name="android:scrollingCache">false</item>
        <item name="android:divider">@drawable/bm_dottedline_bg</item>
        <item name="android:cacheColorHint">@android:color/transparent</item>
        <item name="android:listSelector">@android:color/transparent</item>
    </style>
    
</resources>